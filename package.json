{
  "name": "archbot",
  "version": "2.1.0",
  "description": "Bot for discord",
  "main": "dist/archbot.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "npx cross-env NODE_ENV=production node .",
    "dev": "cross-env NODE_ENV=dev ts-node archbot.ts",
    "serve": "npx cross-env NODE_ENV=production ts-node archbot.ts --transpileOnly",
    "build": "npx rimraf dist && tsc && npm run minify && npm run copydata",
    "minify": "node mterser --glob dist/**/*.js --compress --mangle",
    "copydata": "npx copyfiles plugins/**/*.png dist && npx copyfiles -f assets/* dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justin-m-lacy/archbot.git"
  },
  "author": "Lerping Lemur",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justin-m-lacy/archbot/issues"
  },
  "homepage": "https://github.com/justin-m-lacy/archbot#readme",
  "dependencies": {
    "archcache": "^1.1.0",
    "archdice": "^1.1.0",
    "bufferutil": "^4.0.3",
    "chargrid": "0.0.3",
    "chess-rules": "^1.1.0",
    "discord.js": "^13.8.0",
    "formulic": "^2.0.3",
    "jimp": "^0.16.0"
  },
  "keywords": [
    "discord",
    "discordjs",
    "bot",
    "games",
    "fun",
    "chat",
    "node",
    "8ball",
    "rpg"
  ],
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "glob": "^8.0.1",
    "rimraf": "^3.0.2",
    "terser": "^5.13.1",
    "ts-node": "^10.7.0"
  }
}
